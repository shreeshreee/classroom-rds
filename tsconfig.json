/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "~/*": [
        "src/*"
      ],
      "@utils/*": [
        "src/app/shared/utils/*"
      ],
      "@shared/*": [
        "./src/app/shared/*"
      ],
      "@animations/*": [
        "./src/app/shared/animations/*"
      ],
      "@core/*": [
        "./src/app/core/*"
      ],
      "@api/*": [
        "./src/app/core/api/*"
      ],
      "@resolvers/*": [
        "./src/app/core/resolvers/*"
      ],
      "@store/*": [
        "./src/app/core/store/*"
      ],
      "@env/*": [
        "./projects/gapi-rds/src/environments/*"
      ]
    },
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "module": "es2020",
    "lib": [
      "es2018",
      "dom"
    ]
  }
}
