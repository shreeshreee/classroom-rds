{
  "name": "classroom-rds",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prod": "ng build --prod",
    "deploy": "ng build --prod && npm run doc:g && firebase deploy",
    "doc:g": "npx @compodoc/compodoc -p ./tsconfig.json --output ./docs --name \"Google Classroom RDS Admin\" --customFavicon ./projects/classroom-rds/src/assets/icons/favicon.ico --customLogo ./projects/classroom-rds/src/assets/images/rds-newlogo-transparent.png --theme material --hideGenerator --language es-ES",
    "pwa:assets-std-logo": "npx pwa-asset-generator ./projects/classroom-rds/src/assets/images/rds-newlogo-transparent.png ./projects/classroom-rds/src/assets/icons/ --manifest ./projects/classroom-rds/src/manifest.webmanifest --index ./projects/classroom-rds/src/index.html --type png --maskable true --opaque false --mstile --favicon",
    "pwa:assets-icons": "npx pwa-asset-generator ./projects/classroom-rds/src/assets/images/rds-newlogo-transparent.png ./projects/classroom-rds/src/assets/icons/ --only-icons --index ./projects/classroom-rds/src/index.html --favicon"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.9",
    "@angular/cdk": "^11.0.4",
    "@angular/common": "~11.0.9",
    "@angular/compiler": "~11.0.9",
    "@angular/core": "~11.0.9",
    "@angular/fire": "^6.1.4",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "~11.0.9",
    "@angular/material": "^11.0.4",
    "@angular/platform-browser": "~11.0.9",
    "@angular/platform-browser-dynamic": "~11.0.9",
    "@angular/platform-server": "~11.0.9",
    "@angular/router": "~11.0.9",
    "@angular/service-worker": "~11.0.9",
    "@fortawesome/angular-fontawesome": "~0.8.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-regular-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@ngrx/data": "^10.1.2",
    "@ngrx/effects": "^10.1.2",
    "@ngrx/entity": "^10.1.2",
    "@ngrx/router-store": "^10.1.2",
    "@ngrx/store": "^10.1.2",
    "@ngrx/store-devtools": "^10.1.2",
    "bootstrap": "4.5.3",
    "firebase": "^8.2.3",
